#!/usr/bin/env node
var program = require('commander');

program
  .command('network-list', 'List networks')
  .command('network-create', 'Create new network')
  .command('network-destroy', 'Destroy a network')
  .command('subnet-list', 'List subnets')
  .command('subnet-create', 'Create new subnet')
  .command('subnet-destroy', 'Destroy a subnet')
  .parse(process.argv);
